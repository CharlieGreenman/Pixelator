input#open-ps.opener(type="checkbox", name="open-ps")
label#openpresets.start(for="open-ps")
aside#ps.presets.start
  #presets
    h3.ps-title
      | Presets
      button#add-ps(onclick="addPreset()") +
    #actualcolor Color selected
    div
      button#select-1(onclick="selectPreset(this)") 1
      input#preset-1.color(type="text", name="preset-1", value="#1abc9c")
    div
      button#select-2(onclick="selectPreset(this)") 2
      input#preset-2.color(type="text", name="preset-2", value="#3498db")
    div
      button#select-3(onclick="selectPreset(this)") 3
      input#preset-3.color(type="text", name="preset-3", value="#9b59b6")
    div
      button#select-4(onclick="selectPreset(this)") 4
      input#preset-4.color(type="text", name="preset-4", value="#e74c3c")
    div
      button#select-5(onclick="selectPreset(this)") 5
      input#preset-5.color(type="text", name="preset-5", value="#f39c12")
